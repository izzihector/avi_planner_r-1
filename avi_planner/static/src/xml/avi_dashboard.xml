<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
	<t t-name="avi_planner.dashboard">
     <style type="text/css" t-if="! widget.isFirefox">
            .o_control_panel.o_breadcrumb_full, .oe_view_nocontent {
                display: none;
            }
        </style>
           <div class="col-md-12" style="padding-top: 16px">
              <form class="form-inline">
                <div class="form-group col-md-6">
                  <h3><label for="granjas">Granja</label></h3>
                  <select id="granjas" class="btn">
                     <option value="volvo">CUARTELES</option>
                      <option value="saab">BUEYERA</option>
                      <option value="mercedes">BUEYERA 2</option>
                  </select>
                </div>
                <div class="form-group col-md-6">
                  <h3><label for="parvadas">Parvada</label></h3>
                  <select id="parvadas" class="btn">
                    <option value="volvo">105</option>
                    <option value="volvo">106</option>
                    <option value="volvo">107</option>
                  </select>
                </div>
              </form>
           </div>
        <!--Dashboard buttons-->
        <div class="col-xs-12 o_box" style="padding-top: 25px;">
                        <div class="col-md-2 col-sm-2 hr_payslip">
                            <div class="card">
                                <div class="card-body">
                                    <div class="stat-widget-one">
                                        <div class="stat-icon dib"><i class="fas fa-kiwi-bird"/></div>
                                        <div class="stat-content dib">
                                            <div class="stat-text">Ave Recibida</div>
                                            <div class="stat-digit">1</div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>  
                        <div class="col-md-2 col-sm-2">
                          <div class="card">
                              <div class="card-body">
                                  <div class="stat-widget-one">
                                      <div class="stat-icon dib"><i class="fas fa-kiwi-bird"/></div>
                                      <div class="stat-content dib">
                                          <div class="stat-text">Ave Enviada Posturas</div>
                                          <div class="stat-digit">1</div>
                                      </div>
                                  </div>
                              </div>
                          </div>
                        </div> 
                        <div class="col-md-2 col-sm-2">
                          <div class="card">
                              <div class="card-body">
                                  <div class="stat-widget-one">
                                      <div class="stat-icon dib"><i class="fas fa-book-dead"/></div>
                                      <div class="stat-content dib">
                                          <div class="stat-text">Mortalidad Total</div>
                                          <div class="stat-digit">1</div>
                                      </div>
                                  </div>
                              </div>
                          </div>
                        </div>  
                        <div class="col-md-2 col-sm-2">
                          <div class="card">
                              <div class="card-body">
                                  <div class="stat-widget-one">
                                      <div class="stat-icon dib"><i class="fas fa-balance-scale"/></div>
                                      <div class="stat-content dib">
                                          <div class="stat-text">Diferencia de Aves</div>
                                          <div class="stat-digit">1</div>
                                      </div>
                                  </div>
                              </div>
                          </div>
                        </div>                 
                        <div class="col-md-2 col-sm-2">
                          <div class="card">
                              <div class="card-body">
                                  <div class="stat-widget-one">
                                      <div class="stat-icon dib"><i class="fas fa-book-dead"/></div>
                                      <div class="stat-content dib">
                                          <div class="stat-text">% Mortalidad Acum</div>
                                          <div class="stat-digit">1</div>
                                      </div>
                                  </div>
                              </div>
                          </div>
                        </div>
                        <div class="col-md-2 col-sm-2">
                          <div class="card">
                              <div class="card-body">
                                  <div class="stat-widget-one">
                                      <div class="stat-icon dib"><i class="fas fa-book-dead"/></div>
                                      <div class="stat-content dib">
                                          <div class="stat-text">% Mortalidad al Cierre</div>
                                          <div class="stat-digit">1</div>
                                      </div>
                                  </div>
                              </div>
                          </div>
                        </div>  
                        <div class="col-md-2 col-sm-2">
                          <div class="card">
                              <div class="card-body">
                                  <div class="stat-widget-one">
                                      <div class="stat-icon dib"><i class="fas fa-truck-moving"/></div>
                                      <div class="stat-content dib">
                                          <div class="stat-text">Kgs. Alimento Enviado</div>
                                          <div class="stat-digit">1</div>
                                      </div>
                                  </div>
                              </div>
                          </div>
                        </div> 
                        <div class="col-md-2 col-sm-2">
                          <div class="card">
                              <div class="card-body">
                                  <div class="stat-widget-one">
                                      <div class="stat-icon dib"><i class="fas fa-truck"/></div>
                                      <div class="stat-content dib">
                                          <div class="stat-text">Kgs. Alimento Consumido</div>
                                          <div class="stat-digit">1</div>
                                      </div>
                                  </div>
                              </div>
                          </div>
                        </div> 
                        <div class="col-md-2 col-sm-2">
                          <div class="card">
                              <div class="card-body">
                                  <div class="stat-widget-one">
                                      <div class="stat-icon dib"><i class="fas fa-truck"/></div>
                                      <div class="stat-content dib">
                                          <div class="stat-text">Grs. Consumidos Enviados</div>
                                          <div class="stat-digit">1</div>
                                      </div>
                                  </div>
                              </div>
                          </div>
                        </div> 
                        <div class="col-md-2 col-sm-2">
                          <div class="card">
                              <div class="card-body">
                                  <div class="stat-widget-one">
                                      <div class="stat-icon dib"><i class="fas fa-truck"/></div>
                                      <div class="stat-content dib">
                                          <div class="stat-text">Grs. Consumidos Servidos</div>
                                          <div class="stat-digit">1</div>
                                      </div>
                                  </div>
                              </div>
                          </div>
                        </div>                  
          </div>
        <div class="container-fluid">
                  <div class="row">
                      <div class="col-md-7">
                          <div class="panel">
                              <div class="panel-heading">
                                  <h3 class="panel-title">Consumo Grs. Ave Acumulado</h3>
                              </div>
                              <div class="panel-body">
                                    <canvas id="Chetos" width="300px" height="200px"></canvas>
                              </div>
                          </div>
                      </div>
              </div>
  </div>

        
</t>
</templates>
